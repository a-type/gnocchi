	},
	"dependencies": {
		"@aglio/api": "workspace:^0.0.1",
		"@aglio/conversion": "workspace:^0.0.1",
		"@aglio/react": "workspace:^0.0.1",
		"@aglio/storage-react": "workspace:^0.0.1",
		"@dnd-kit/utilities": "^3.2.0",
		"@types/ws": "^8.5.3",
		"react": "^18.2.0",
