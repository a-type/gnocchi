{
	"name": "@aglio/groceries-client",
	"version": "0.0.1",
	"private": true,
	"exports": {
		".": {
			"prod": "./dist/esm/index.js",
			"import": "./src/index.ts",
			"node": "./dist/esm/index.js",
			"default": "./dist/esm/index.js"
		}
	},
	"type": "module",
	"scripts": {
		"generate": "verdant -s ./src/schema.ts -o ./src/client -r",
		"link-verdant": "pnpm link --global @verdant-web/store && pnpm link --global @verdant-web/cli && pnpm link --global @verdant-web/common && pnpm link --global @verdant-web/react",
		"preflight": "verdant preflight -s ./src/schema.ts -o ./src/client",
		"build": "tsc && node ./scripts/copyClient.js"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"dependencies": {
		"@aglio/trpc": "workspace:*",
		"@trpc/client": "^10.10.0",
		"@trpc/server": "^10.10.0",
		"@verdant-web/cli": "^4.2.0",
		"@verdant-web/common": "2.0.0",
		"@verdant-web/react": "25.0.0",
		"@verdant-web/store": "3.0.0",
		"cuid": "^2.1.8",
		"stopword": "^2.0.8"
	},
	"devDependencies": {
		"@types/stopword": "^2.0.0",
		"react": "^18.2.0"
	}
}
