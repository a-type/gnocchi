{
	"$schema": "https://json.schemastore.org/web-manifest-combined.json",
	"name": "Aglio",
	"short_name": "Aglio",
	"description": "A tiny groceries app",
	"background_color": "#ffffff",
	"theme_color": "#f7d352",
	"icons": [
		{
			"src": "/android-chrome-192x192.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "/android-chrome-512x512.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	],
	"display": "standalone",
	"start_url": "/",
	"share_target": {
		"action": "/share",
		"method": "POST",
		"enctype": "multipart/form-data",
		"params": {
			"title": "title",
			"text": "text",
			"url": "url"
		}
	}
}
