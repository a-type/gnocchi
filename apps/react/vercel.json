{
	"headers": [
		{
			"source": "(.*)?",
			"headers": [
				{
					"key": "Cross-Origin-Opener-Policy",
					"value": "same-origin"
				},
				{
					"key": "Cross-Origin-Embedder-Policy",
					"value": "require-corp"
				}
			]
		}
	],
	"routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }]
}
